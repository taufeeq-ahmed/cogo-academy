---
import CourseCard from "./CourseCard.astro";

const {course} = Astro.props
const response = await fetch(
  `${import.meta.env.PUBLIC_SERVER_URL}/${course}/sections`,
);
const data = await response.json();
---
{
    data?.map((sec) => {
        return (
            <CourseCard section={sec} />
        );
    })
}