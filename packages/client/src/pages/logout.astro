<div>Logging out</div>
<script>
    // document.cookie = "cogoacademytoken=; expires=Thu, 01 Jan 1970 00:00:00 UTC; path=/;";
    document.cookie.split(";").forEach(function (c) {
        document.cookie = c
            .replace(/^ +/, "")
            .replace(
                /=.*/,
                "=;expires=" + new Date().toUTCString() + ";path=/"
            );
    });
    window.location.href = "/signin";
</script>
